{
  "name": "@builder.io/qwik-city",
  "version": "0.0.13",
  "description": "Static Site Generator for Qwik",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "qwik": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build.runtime": "cd runtime && vite build --mode lib",
    "test.unit": "uvu -r tsm src \"(unit)\"",
    "test.build": "yarn test.build.client && yarn test.build.ssr",
    "test.build.client": "cd src/test && vite build",
    "test.build.ssr": "cd src/test && vite build --ssr src/entry.express.tsx",
    "test.serve": "node --inspect-brk src/test/server/entry.express.js",
    "test.serve.debug": "node --inspect-brk src/test/server/entry.express.js"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.1",
    "@types/mdx": "2.0.2",
    "source-map": "^0.7.3",
    "vfile": "^5.3.2"
  },
  "peerDependencies": {
    "@builder.io/qwik": "workspace:*"
  },
  "devDependencies": {
    "@builder.io/qwik": "workspace:*",
    "@microsoft/api-extractor": "7.24.2",
    "@types/github-slugger": "^1.3.0",
    "@types/marked": "^4.0.3",
    "@types/node": "latest",
    "@types/refractor": "^3.0.2",
    "estree-util-value-to-estree": "^1.3.0",
    "github-slugger": "^1.4.0",
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-to-string": "^2.0.0",
    "marked": "^4.0.16",
    "refractor": "^4.7.0",
    "rehype-autolink-headings": "^6.1.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "tsm": "^2.2.1",
    "typescript": "4.7.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0",
    "uvu": "^0.5.3",
    "vite": "^2.9.12"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cloudflare-pages": {
      "import": "./dist/cloudflare-pages/index.mjs"
    },
    "./express": {
      "import": "./dist/express/index.mjs",
      "require": "./dist/express/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://qwik.builder.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderIO/qwik.git",
    "directory": "packages/qwik-city"
  },
  "bugs": {
    "url": "https://github.com/BuilderIO/qwik/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
