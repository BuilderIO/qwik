{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2021",
    "module": "ES2020",
    "lib": ["es2021", "DOM"],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": true,
    "declaration": true,
    "declarationDir": "lib-types",
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "isolatedModules": true,
    "types": [
      "vite/client"
    ],
    "paths": {
      "@qwik-client-manifest": [
        "../../qwik/src/server/server-modules.d.ts"
      ],
      "@builder.io/qwik/jsx-runtime": [
        "../../qwik/src/jsx-runtime/index.ts"
      ],
      "@builder.io/qwik/jsx-dev-runtime": [
        "../../qwik/src/jsx-runtime/index.ts"
      ],
    }
  },
  "include": ["src"]
}
