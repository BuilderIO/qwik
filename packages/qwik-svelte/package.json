{
  "name": "@builder.io/qwik-svelte",
  "version": "0.1.1",
  "description": "Qwik Svelte allows you to add Svelte components into an existing Qwik application",
  "scripts": {
    "build": "npm run build.lib",
    "build.client": "vite build",
    "build.lib": "vite build --mode lib",
    "build.ssr": "vite build --ssr src/entry.ssr.tsx",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "dev": "vite",
    "dev:example": "vite --config vite.config.example",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "release": "np",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "workspace:^",
    "@builder.io/qwik-city": "workspace:^",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@types/react": "^18.0.24",
    "react": "^18.2.0",
    "svelte": "^3.52.0",
    "typescript": "4.8.4",
    "vite": "3.2.1"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">=0.1.11",
    "@builder.io/qwik-city": ">=0.1.118",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "svelte": "^3.52.0"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib/types/index.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "license": "MIT",
  "homepage": "https://qwik.builder.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderIO/qwik.git",
    "directory": "packages/qwik-svelte"
  },
  "bugs": {
    "url": "https://github.com/BuilderIO/qwik/issues"
  },
  "type": "module",
  "main": "./lib/index.qwik.mjs",
  "module": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib/types/index.d.ts"
}
