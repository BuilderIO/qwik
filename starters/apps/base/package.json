{
  "name": "qwik-project-name",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run typecheck && npm run build.client && npm run build.server",
    "build.client": "vite build",
    "build.server": "vite build --ssr src/entry.server.tsx",
    "dev": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --force --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "serve": "node server/entry.server.mjs",
    "typecheck": "tsc --incremental --noEmit",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "latest",
    "@builder.io/qwik-city": "0.0.109",
    "@types/eslint": "8.4.1",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint-plugin-qwik": "latest",
    "eslint": "8.12.0",
    "node-fetch": "latest",
    "prettier": "latest",
    "polka": "latest",
    "sirv": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "3.5.0"
  },
  "type": "module",
  "private": true,
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
