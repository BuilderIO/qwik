{
  "name": "qwik-site",
  "version": "0.0.0",
  "description": "Qwik Site.",
  "scripts": {
    "build": "npm run build.client && npm run build.server",
    "build.client": "vite build",
    "build.server": "vite build --outDir server/build --ssr src/entry.cloudflare.tsx",
    "build.qwest": "cd qwest && npm run build",
    "preview": "vite preview",
    "dev": "npm run dev.ssr",
    "dev.ssr": "vite",
    "dev.client": "vite --mode clientonly",
    "dev.wrangler": "wrangler pages dev ./dist",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "start": "npm run dev",
    "link.builder": "npm link @builder.io/qwik",
    "postinstall": "cd qwest && npm install"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.5.0",
    "@builder.io/qwik": "0.0.18-2",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@cloudflare/workers-types": "^3.2.0",
    "@mdx-js/mdx": "^2.0.0",
    "@types/service-worker-mock": "2.0.1",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.6",
    "prettier": "2.3.0",
    "source-map": "^0.7.3",
    "tailwindcss": "3.0.18",
    "tsm": "^2.2.1",
    "typescript": "^4.5.2",
    "vfile": "^5.3.2",
    "vite": "^2.7.13",
    "wrangler": "beta"
  },
  "author": "Builder.io Team",
  "engines": {
    "node": ">=14.14.0"
  },
  "license": "MIT"
}
